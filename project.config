[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Land Maintainers
	owner = group user/Tab Fitts (tabp0le)
[access "refs/for/refs/*"]
	read = group Land Maintainers
	read = group user/Tab Fitts (tabp0le)
	push = group Land Maintainers
	push = group user/Tab Fitts (tabp0le)
	label-Code-Review = -2..+2 group Land Maintainers
	label-Code-Review = -2..+2 group user/Tab Fitts (tabp0le)
	label-Verified = -1..+1 group Land Maintainers
	label-Verified = -1..+1 group user/Tab Fitts (tabp0le)
	rebase = group Land Maintainers
	rebase = group user/Tab Fitts (tabp0le)
	removeReviewer = group Land Maintainers
	removeReviewer = group user/Tab Fitts (tabp0le)
	submit = group Land Maintainers
	submit = group user/Tab Fitts (tabp0le)
[access "refs/heads/*"]
	read = group Land Maintainers
	read = group user/Tab Fitts (tabp0le)
	create = group Land Maintainers
	create = group user/Tab Fitts (tabp0le)
	push = group Land Maintainers
	push = +force group user/Tab Fitts (tabp0le)
	pushMerge = group Land Maintainers
	pushMerge = group user/Tab Fitts (tabp0le)
[access "refs/tags/*"]
	push = +force group Land Maintainers
	push = group user/Tab Fitts (tabp0le)
	push = +force group user/Tab Fitts (tabp0le)
	label-Verified = -1..+1 group Land Maintainers
[submit]
	action = rebase if necessary
